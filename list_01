#include <iostream>
using namespace std;
struct node {
	int value;
	node * next;
};

void add(node *begin, int a) {
	node* j = new node;
	j->next = begin;
	j->value = a;
	while (begin) {
		if (begin->next == NULL) {
			begin->next = j;
			j->next = NULL;
			break;
		}
		begin = begin->next;
	}
}

void Print(node *m) {
	node *print = m;
	while (print) {
		cout << print->value << " -> ";
		print = print->next;
	}
	cout << "NULL" << endl;
}

void Del(node *begin) {
	//node* j = new node;
	//j = begin;
	//begin->next = begin;
	//begin->next->next = begin->next;
	//delete begin;
	//begin = j;
	//delete begin;
	//begin->next = begin->next->next;
	/*while (begin->next) {
		if (begin->next->next == 0) {
			begin->next = 0;
		}
		else {
			begin = begin->next;
		}
	}*/
}
char op;
int main()
{
	int value[5] = { 1,2,3,4,5 };
	int a; char q=0;
	node* begin = NULL;
	begin = new node;
	begin->value = value[0];
	node* end = begin;
	for (int i = 1; i < 5; i++) {
		
		end->next = new node;
		end = end->next;
		end->value = value[i];
		end->next = NULL;
	}
	while (op != 'q') {
		cin >> op;
		switch (op) {
		case '+':
			cin >> a;
			add(begin, a);
			Print(begin);
			break;
		case '=':
			Print(begin);
			break;
		case '-':
			Del(begin);
			Print(begin);
			break;
		}
	}
    return 0;
}
